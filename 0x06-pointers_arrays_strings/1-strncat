#include "main.h"
/**
 * _strcat - concatenates two strings
 * @dest: String to be appended
 * @src: String to be appended to dest
 * @n: number of bytes
 * Return: pointer of resulting dest
 */
char *_strcat(char *dest, char *src, int n)
{
	int i = 0, lenght = 0;

	while (dest[i])
	{
		lenght++;
	}
	for (i = 0; src[i] && i < n; i++)
	{
		dest[length++] = src[i];
	}
	return (0);
}
